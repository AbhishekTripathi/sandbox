{
  "build": {
    "content": [{
        "group": "all",
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "group": "unity",
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [{
        "group": "all",
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "group": "unity",
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/sandbox/breadcrumb/toc.json"
    },
    "fileMetadata": {
      "all": {},
      "monikerRange": {
        "sandbox/gamedev/unity/**/*.md": ">= unity-2017.1"
      }
    },
    "groups": {
      "all": {},
      "unity": {
        "moniker_range": ">= unity-2017.1",
        "dest": "sandbox/gamedev/unity/"
      }
    },
    "template": [],
    "dest": "sandbox"
  }
}